enum ChangeType {

  definition {
    The type of change described in AmendBlock.
  }

  add {
    definition {
      Indicates a change that adds content.
    }
  }
  modify {
    definition {
      Indicates a change that modifies existing content.
    }
  }
  delete {
    definition {
      Indicates a change that deletes content.
    }
  }
}
