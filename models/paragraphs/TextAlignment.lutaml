enum TextAlignment {
  definition {
    The alignment of the paragraph against the margins of the document.
  }

  left {
    definition {
      Left alignment.
    }
  }
  center {
    definition {
      Center alignment.
    }
  }
  right {
    definition {
      Right alignment.
    }
  }
  justified {
    definition {
      Justified alignment.
    }
  }
}

